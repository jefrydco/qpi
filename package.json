{
  "name": "qpi",
  "version": "0.1.0",
  "description": "A simple yet relatively powerful http server framework",
  "main": "srcs",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles",
    "test:ci": "yarn test ; SUCCESS=$? ; codecov -t $CODECOV_TOKEN ; return $SUCCESS",
    "lint": "eslint srcs",
    "doc": "jsdoc -c jsdoc.js"
  },
  "dependencies": {
    "restify": "^8.4.0",
    "restify-cors-middleware": "^1.1.1"
  },
  "devDependencies": {
    "codecov": "3.6.1",
    "docdash": "1.1.1",
    "eslint": "6.7.1",
    "eslint-plugin-jest": "23.1.0",
    "jest": "24.9.0",
    "jsdoc": "3.6.3",
    "supertest": "4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qfoooo/qpi.git"
  },
  "keywords": [
    "server",
    "api",
    "cors",
    "http"
  ],
  "author": "qfoooo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qfoooo/qpi/issues"
  },
  "homepage": "https://github.com/qfoooo/qpi#readme"
